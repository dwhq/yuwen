{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "aws/aws-sdk-php": "~3.0",
        "beyondcode/laravel-confirm-email": "^1.3",
        "fideloper/proxy": "~4.0",
        "fukuball/jieba-php": "^0.33.0",
        "guzzlehttp/guzzle": "^6.3",
        "johnlui/aliyun-oss": "~2.0",
        "khanamiryan/qrcode-detector-decoder": "^1.0",
        "laravel/framework": "5.6.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelchen/myflash": "^1.3",
        "mews/captcha": "^2.1",
        "noisywinds/laravel-smartmd": "^1.0",
        "sebastian/environment": "^3.1",
        "simplesoftwareio/simple-qrcode": "1.3.*",
        "socialiteproviders/qq": "^3.0",
        "stevenyangecho/laravel-u-editor": "~1.4",
        "vanry/laravel-scout-tntsearch": "^1.2"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories",
            "app/services"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Libraries/Functions/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
